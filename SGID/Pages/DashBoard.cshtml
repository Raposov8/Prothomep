@page "{id}"
@model SGID.Pages.DashBoardModel
@{
    ViewData["Title"] = "DASHBOARD";
    if (User.IsInRole("Admin"))
    {
        Layout = "_LayoutNovo";
    }
    else if (User.IsInRole("Diretoria"))
    {
        Layout = "_LayoutDiretoria";
    }
    else
    {
        Layout = "_LayoutNovo";
    }
}

